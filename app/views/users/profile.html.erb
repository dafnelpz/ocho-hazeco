<div id="general-info">

	<div id="user-info">
		<div id="profile-pic"></div>
		<div id ="profile-info">
			<p id="c-user-name"><span><%= current_user.name  %><span></p>
			<p id="c-user-agent"> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Tu agente es <span class="agent-name"><%= current_user.agent.name  %></span></p>
			<p id="c-user-agent">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;📞 <%= current_user.agent.phone  %></p>
			<p id="c-user-agent">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;📧 <%= current_user.agent.email  %></p>
		</div>
	</div>

	<div id="profile-benefits">
		<h4>Reciclando a distancia has logrado...</h4></br>
		<p>Ahorrar <span class="benefit bnft-1">215 lts</span>  de agua</p>
		<p>Ahorrar <span class="benefit bnft-2">3000 watts</span> de energía</p>
		<p>Salvar <span class="benefit bnft-3">20 especies</span> marinas</p>
	</div>
</div>

<div id="profile-deliveries">
	<div id="last-delivery">
		<h5><span class="espan"><span class="benefit">La última vez</span> entregaste</span></h5>
		<div id="table-last">
		</div>
		<div id="comments-last">
		</div>
	</div>
	<div id="total-deliveries">
		<h5><span class="espan"><span class="benefit">En total</span> has entregado</span></h5>
		<div id="table-total">
			<%= javascript_include_tag "https://www.google.com/jsapi", "chartkick" %>
 			<% my_chart = {"Bolsa azul": 45, "Bolsa naranja": 35, "Bolsa morada": 20} %>
 			<%= pie_chart my_chart %>
		</div>
	</div>
</div>

